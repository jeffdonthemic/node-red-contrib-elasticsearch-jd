      "create": "create.js",
      "exists": "exists.js",
      "update": "update.js",
      "delete": "delete.js",
      "get": "get.js",
      "bulk": "bulk.js",
      "remote-server": "remote-server.js"
    }
  },
  "license": "MIT",
  "dependencies": {
    "elasticsearch": ">=1.1.0"
